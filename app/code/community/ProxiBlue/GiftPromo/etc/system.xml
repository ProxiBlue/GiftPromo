<config>
    <tabs>
        <proxiblue translate="label" module="giftpromo">
            <label><![CDATA[<div style="background:url(data:image/png;base64,) no-repeat; width: 155px; height: 20px;"></div>]]></label>
            <sort_order>10000000</sort_order>
        </proxiblue>
    </tabs>
    <sections>
        <giftpromo translate="label" module="giftpromo">
            <label><![CDATA[<div style="background:url(//www.proxiblue.com.au/moduleicons/fetch/icon/name/giftpromo) no-repeat; background-position:left; padding-left: 20px;">Gift Promotions</div>]]></label>
            <tab>proxiblue</tab>
            <frontend_type>text</frontend_type>
            <sort_order>999999</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <hint>
                    <frontend_model>giftpromo/adminhtml_system_config_fieldset_hint</frontend_model>
                    <sort_order>0</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                </hint>
                <catalog translate="label">
                    <label>Catalog</label>
                    <sort_order>1</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <icons_enabled translate="label">
                            <label>Display Gift Icons?</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </icons_enabled>
                    </fields>
                </catalog>
                <cart translate="label">
                    <label>Cart</label>
                    <sort_order>1</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <oos_enabled translate="label">
                            <label>Display Out Of Stock Gifts?</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </oos_enabled>
                        <selectgifts_message translate="label">
                            <label>Message in cart for selecable gifts</label>
                            <frontend_type>textarea</frontend_type>
                            <sort_order>4</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>
                                <![CDATA[The heading that appears in the cart, upon select gifts being available
                                        <br/>Placeholders:<ul><li>{GIFTING_PRODUCT}: The name of the product that caused the gifting.</li>
                                        <li>{RULE_NAME}: The name of the rule that effected the gifting.</li>
                                        <li>{RULE_DESCRIPTION}: The description of the rule that effected the gifting.</li>
                                        <li>{SELECT_COUNT}: Display the numeric number of total items that can be selected.</li>
                                        </ul>]]>
                            </comment>
                        </selectgifts_message>
                        <selectitems_bestfit>
                            <label>Try 'Best Fit' on selectable gift items display</label>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <frontend_type>select</frontend_type>
                            <sort_order>6</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>
                                <![CDATA[Requires jQuery to be loaded. Not provided by this extension]]>
                            </comment>
                        </selectitems_bestfit>
                        <composite_message>
                            <label>Composite Message Display</label>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <frontend_type>select</frontend_type>
                            <sort_order>6</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>
                                <![CDATA[Enable the message when composite products trigger gifting. (This gift added for....)]]>
                            </comment>
                        </composite_message>
                        <selectitems_multiselect>
                            <label>Use checkboxes for multiple gift select in selectable lists</label>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <frontend_type>select</frontend_type>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>
                                <![CDATA[Only works with simple product types. Other product types will display add to cart button]]>
                            </comment>
                        </selectitems_multiselect>
                        <selectitems_nolink_notvis>
                            <label>Do not place links in selectable gifts, if item is not visible</label>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <frontend_type>select</frontend_type>
                            <sort_order>11</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>
                                <![CDATA[If a product is not visible, do not allow it to link to product view]]>
                            </comment>
                        </selectitems_nolink_notvis>
                        <use_alternative_items_count translate="label">
                            <label>Use alternative method to calculate line item count in cart</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>
                                <![CDATA[Enable this to ignore gift items in quantity calculation
                                <br/>It has also been found that quantities can be doubled. Enabling this option fixes that issue as well]]>
                            </comment>
                        </use_alternative_items_count>
                    </fields>
                </cart>
                <checkout translate="label">
                    <label>Checkout</label>
                    <sort_order>1</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <review_select_disabled translate="label">
                            <label>Hide selectable gifts in checkout review?</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>Set to yes to disable selectable gifts in order review during checkout. <br/> If you use any of the checkout rules, and have selectable gifts, this must be enabled!</comment>
                        </review_select_disabled>
                    </fields>
                </checkout>
                <orders translate="label">
                    <label>Orders</label>
                    <sort_order>1</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <last_ordered_enabled translate="label">
                            <label>Hide Gift Items from Last Ordered Items</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </last_ordered_enabled>
                    </fields>
                </orders>
                <api translate="label">
                    <label>Api</label>
                    <sort_order>999</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <strip_gift_type translate="label">
                            <label>Compatible product types on export</label>
                            <comment><![CDATA[If you us ethe API to transfer products to external systems,
                            they might not cope with the gift products types.<br/>
                            Try this option to attempt to make it compatible.]]></comment>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </strip_gift_type>
                    </fields>
                </api>
                <debug translate="label">
                    <label>Debug Options</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>10000</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <enabled translate="label">
                            <label>Enabled debug mode</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </enabled>
                        <level translate="label">
                            <label>Level of debug</label>
                            <comment><![CDATA[The debug level 1-5]]></comment>
                            <frontend_type>text</frontend_type>
                            <sort_order>2</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </level>
                        <trace translate="label">
                            <label>Enabled debug trace</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </trace>
                        <conflicts>
                            <frontend_model>giftpromo/adminhtml_system_config_fieldset_conflicts</frontend_model>
                            <sort_order>99999</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </conflicts>
                    </fields>
                </debug>
            </groups>
        </giftpromo>
    </sections>

</config>